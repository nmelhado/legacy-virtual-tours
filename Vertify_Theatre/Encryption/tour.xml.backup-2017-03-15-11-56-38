<action name="openPhoto">
	if(layer[Photo_holder].status == 1,
			set(layer[Photo_holder].visible,false);
			set(layer[PhotoBackground].visible,false);
			set(layer[Photo_holder].status,0);
			,
			set(layer[Study].url,Ref/Images/Dome_Study1.jpg);
			set(layer[Study].current,1);
			set(layer[nav_right].visible,true);
			set(layer[nav_left].visible,false);
			set(layer[Photo_holder].visible,true);
			set(layer[PhotoBackground].visible,true);
			set(layer[Photo_holder].status,1);
	);
</action>

<action name="slide">
	if(%1 == 1,
			if(layer[Study].current LT 13,
				set(layer[Study].visible,false);
				inc(layer[Study].current,1);
				set(layer[nav_left].visible,true);
				if(layer[Study].current == 13, set(layer[nav_right].visible,false););
				txtadd(purl,'Ref/Images/Dome_Study',get(layer[Study].current),'.jpg');
				set(layer[Study].url,get(purl));
				delayedcall(0,set(layer[Study].visible,true););
			);
			,
			if(layer[Study].current GT 1,
				set(layer[Study].visible,false);
				inc(layer[Study].current,-1);
				set(layer[nav_right].visible,true);
				if(layer[Study].current == 1, set(layer[nav_left].visible,false););
				txtadd(purl,'Ref/Images/Dome_Study',get(layer[Study].current),'.jpg');
				set(layer[Study].url,get(purl));
				delayedcall(0,set(layer[Study].visible,true););
			);
	);
</action>
	
