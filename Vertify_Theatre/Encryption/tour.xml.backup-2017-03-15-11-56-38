<action name="overlay_alpha">
		if(%1 != dragging,
			copy(drag_currenty, y);
			copy(drag_stagey, mouse.stagey); 
			div(adj, layer[Overlay_Parent].imageheight, stageheight);
			overlay_alpha(dragging);
			,
			if(pressed,
				sub(dy, mouse.stagey, drag_stagey);
				mul(dy, adj);
				mul(dy,-1);
				add(dy, drag_currenty);
				if(dy LT 0, set(dy,0););
				if(dy GT 770, set(dy,770););
				div(2dy, dy, 770);
				copy(o_alpha,2dy);
				roundval(o_alpha,2);
				sub(o_alpha2,1,o_alpha);
				mul(2dy, 100);
				txtadd(2dy, '%');
				copy(y, dy);
				copy(layer[Status].height,2dy);
				copy(layer[hp].alpha,o_alpha);
				copy(layer[lyric].alpha,o_alpha2);
				copy(hotspot[Overlay_f].alpha,o_alpha);
				copy(hotspot[Overlay_l].alpha,o_alpha);
				copy(hotspot[Overlay_r].alpha,o_alpha);
				copy(hotspot[Overlay_b].alpha,o_alpha);
				copy(hotspot[Overlay_u].alpha,o_alpha);
				copy(hotspot[Overlay_d].alpha,o_alpha);
			);
			delayedcall(0,overlay_alpha(dragging););
		);
</action>

<action name="collapse_overlay">
	if(layer[Overlay_collapse].status == 1,
			set(layer[Overlay_collapse].edge,right);
			set(layer[Overlay_collapse].rotate,180);
			copy(layer[Overlay_collapse].status,0);
			div(adj, stageheight, layer[Overlay_Parent].imageheight);
			mul(adj, layer[Overlay_Parent].imagewidth);
			roundval(adj);
			sub(coll, 0, adj);
			set(layer[Overlay_Parent].x,get(coll));
			,
			set(layer[Overlay_collapse].edge,center);
			set(layer[Overlay_collapse].rotate,0);
			set(layer[Overlay_collapse].status,1);
			set(layer[Overlay_Parent].x,0);
	);
</action>

