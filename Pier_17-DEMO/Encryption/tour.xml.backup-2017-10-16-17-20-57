	<action name="map_level" >
		if(%1 == 1,
			set(layer[NOVA_Map_ground].visible,true);
			set(layer[NOVA_Map_Small_ground].visible,true);
			set(layer[NOVA_Map_Level_ground_active].visible,false);
			set(layer[NOVA_Map_Level_ground_inactive].visible,true);

			set(layer[NOVA_Map_3].visible,false);
			set(layer[NOVA_Map_Small_3].visible,false);
			set(layer[NOVA_Map_Level_3_active].visible,true);
			set(layer[NOVA_Map_Level_3_inactive].visible,false);

			set(layer[NOVA_Map_4].visible,false);
			set(layer[NOVA_Map_Small_4].visible,false);
			set(layer[NOVA_Map_Level_4_active].visible,true);
			set(layer[NOVA_Map_Level_4_inactive].visible,false);

			set(layer[NOVA_Map_roof].visible,false);
			set(layer[NOVA_Map_Small_roof].visible,false);
			set(layer[NOVA_Map_Level_roof_active].visible,true);
			set(layer[NOVA_Map_Level_roof_inactive].visible,false);
		);

		if(%1 == 2,
			set(layer[NOVA_Map_ground].visible,false);
			set(layer[NOVA_Map_Small_ground].visible,false);
			set(layer[NOVA_Map_Level_ground_active].visible,true);
			set(layer[NOVA_Map_Level_ground_inactive].visible,false);

			set(layer[NOVA_Map_3].visible,true);
			set(layer[NOVA_Map_Small_3].visible,true);
			set(layer[NOVA_Map_Level_3_active].visible,false);
			set(layer[NOVA_Map_Level_3_inactive].visible,true);

			set(layer[NOVA_Map_4].visible,false);
			set(layer[NOVA_Map_Small_4].visible,false);
			set(layer[NOVA_Map_Level_4_active].visible,true);
			set(layer[NOVA_Map_Level_4_inactive].visible,false);

			set(layer[NOVA_Map_roof].visible,false);
			set(layer[NOVA_Map_Small_roof].visible,false);
			set(layer[NOVA_Map_Level_roof_active].visible,true);
			set(layer[NOVA_Map_Level_roof_inactive].visible,false);
		);

		if(%1 == 3,
			set(layer[NOVA_Map_ground].visible,false);
			set(layer[NOVA_Map_Small_ground].visible,false);
			set(layer[NOVA_Map_Level_ground_active].visible,true);
			set(layer[NOVA_Map_Level_ground_inactive].visible,false);

			set(layer[NOVA_Map_3].visible,false);
			set(layer[NOVA_Map_Small_3].visible,false);
			set(layer[NOVA_Map_Level_3_active].visible,true);
			set(layer[NOVA_Map_Level_3_inactive].visible,false);

			set(layer[NOVA_Map_4].visible,true);
			set(layer[NOVA_Map_Small_4].visible,true);
			set(layer[NOVA_Map_Level_4_active].visible,false);
			set(layer[NOVA_Map_Level_4_inactive].visible,true);

			set(layer[NOVA_Map_roof].visible,false);
			set(layer[NOVA_Map_Small_roof].visible,false);
			set(layer[NOVA_Map_Level_roof_active].visible,true);
			set(layer[NOVA_Map_Level_roof_inactive].visible,false);
		);

		if(%1 == 4,
			set(layer[NOVA_Map_ground].visible,false);
			set(layer[NOVA_Map_Small_ground].visible,false);
			set(layer[NOVA_Map_Level_ground_active].visible,true);
			set(layer[NOVA_Map_Level_ground_inactive].visible,false);

			set(layer[NOVA_Map_3].visible,false);
			set(layer[NOVA_Map_Small_3].visible,false);
			set(layer[NOVA_Map_Level_3_active].visible,true);
			set(layer[NOVA_Map_Level_3_inactive].visible,false);

			set(layer[NOVA_Map_4].visible,false);
			set(layer[NOVA_Map_Small_4].visible,false);
			set(layer[NOVA_Map_Level_4_active].visible,true);
			set(layer[NOVA_Map_Level_4_inactive].visible,false);

			set(layer[NOVA_Map_roof].visible,true);
			set(layer[NOVA_Map_Small_roof].visible,true);
			set(layer[NOVA_Map_Level_roof_active].visible,false);
			set(layer[NOVA_Map_Level_roof_inactive].visible,true);
		);
	</action>
	
	<action name="pin_rot" >
		if(rotate != 89.75,
			add(rotate,.25);
		,
			set(rotate,0);
		);
		delayedcall(0.01,pin_rot(););
	</action>
	
<style	name="pin"
		url="Ref/pin.png"
		visible="true"
		keep="true"
		zorder="15"
		alpha="1.0"
		align="bottomleft"
		edge="center"
		enabled="true"
		handcursor="true"
		scale="0.20"
		onover="tween(scale,0.35,0.3);"
		onout="tween(scale,0.20,0.3);"
		onclick="delayedcall(0.8,loadscene(get(da_scene),null,get(skin_settings.loadscene_flags),get(skin_settings.loadscene_blend));); delayedcall(0.4, set(layer[NOVA_Map].visible,false)); delayedcall(0.4, set(layer[NOVA_Map_Back].visible,false)); "
		rotate="0"
		onhover=""
		onloaded="pin_rot();"
/>

