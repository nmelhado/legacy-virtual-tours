					delayedcall(2.0, set(layer[HOVR_Outer_spot8z].scale_check,2));
				,
					tween(layer[HOVR_Outer_spot8z].scale, 1.22, 2.0);
					delayedcall(2.0, set(layer[HOVR_Outer_spot8z].scale_check,2));
				);
			);
			
			inc(layer[HOVR_Arrows_spot8].frame,1,359,0);
			set(layer[HOVR_Arrows_spot8].rotate,get(layer[HOVR_Arrows_spot8].frame));
		);
		
		if(layer[HOVR_Circle_spot9],
			dec(layer[HOVR_Circle_spot9].frame,1,0,359);
			set(layer[HOVR_Circle_spot9].rotate,get(layer[HOVR_Circle_spot9].frame));
			
			inc(layer[HOVR_Outer_spot9z].frame,1,359,0);
			set(layer[HOVR_Outer_spot9z].rotate,get(layer[HOVR_Outer_spot9z].frame));
			if(layer[HOVR_Outer_spot9z].scale_check GT 1,
				set(layer[HOVR_Outer_spot9z].scale_check,0);
				if(layer[HOVR_Outer_spot9z].scale GT 1.1,
					tween(layer[HOVR_Outer_spot9z].scale, 1.0, 2.0);
					delayedcall(2.0, set(layer[HOVR_Outer_spot9z].scale_check,2));
				,
					tween(layer[HOVR_Outer_spot9z].scale, 1.22, 2.0);
					delayedcall(2.0, set(layer[HOVR_Outer_spot9z].scale_check,2));
				);
			);
			
			inc(layer[HOVR_Arrows_spot9].frame,1,359,0);
			set(layer[HOVR_Arrows_spot9].rotate,get(layer[HOVR_Arrows_spot9].frame));
		);
		
		if(layer[HOVR_Circle_spot10],
			dec(layer[HOVR_Circle_spot10].frame,1,0,359);
			set(layer[HOVR_Circle_spot10].rotate,get(layer[HOVR_Circle_spot10].frame));
			
			inc(layer[HOVR_Outer_spot10z].frame,1,359,0);
			set(layer[HOVR_Outer_spot10z].rotate,get(layer[HOVR_Outer_spot10z].frame));
			if(layer[HOVR_Outer_spot10z].scale_check GT 1,
				set(layer[HOVR_Outer_spot10z].scale_check,0);
				if(layer[HOVR_Outer_spot10z].scale GT 1.1,
					tween(layer[HOVR_Outer_spot10z].scale, 1.0, 2.0);
					delayedcall(2.0, set(layer[HOVR_Outer_spot10z].scale_check,2));
				,
					tween(layer[HOVR_Outer_spot10z].scale, 1.22, 2.0);
					delayedcall(2.0, set(layer[HOVR_Outer_spot10z].scale_check,2));
				);
			);
			
			inc(layer[HOVR_Arrows_spot10].frame,1,359,0);
			set(layer[HOVR_Arrows_spot10].rotate,get(layer[HOVR_Arrows_spot10].frame));
		);

		delayedcall(0.02, hotspot_animate(););    <!-- here you set the speed of the frame change -->
	</action>
	
	<action name="hotspot_outer">
		inc(frame,1,359,0);
		set(rotate,get(frame));
		delayedcall(0.02, hotspot_outer() );    <!-- here you set the speed of the frame change -->
	</action>
	
	<action name="hotspot_loading_arrows">
		inc(frame,1,359,0);
		set(rotate,get(frame));
		delayedcall(0.012, hotspot_loading_arrows() );    <!-- here you set the speed of the frame change -->
	</action>

