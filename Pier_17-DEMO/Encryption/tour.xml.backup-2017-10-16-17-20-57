		if(hotspot[spot9],set(hotspot[spot9].width,14%); set(hotspot[spot9].height,prop););
		if(hotspot[spot10],set(hotspot[spot10].width,14%); set(hotspot[spot10].height,prop););
		if(hotspot[spot11],set(hotspot[spot11].width,14%); set(hotspot[spot11].height,prop););
