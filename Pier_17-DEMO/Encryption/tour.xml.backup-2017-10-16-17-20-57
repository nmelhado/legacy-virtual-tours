<action name="d_resize">

	div(screenaspect, stagewidth, stageheight);
	
	if(screenaspect GT 1,
		if(hotspot[spot0],set(hotspot[spot0].height,14%); set(hotspot[spot0].width,prop););
		if(hotspot[spot1],set(hotspot[spot1].height,14%); set(hotspot[spot1].width,prop););
		if(hotspot[spot2],set(hotspot[spot2].height,14%); set(hotspot[spot2].width,prop););
		if(hotspot[spot3],set(hotspot[spot3].height,14%); set(hotspot[spot3].width,prop););
		if(hotspot[spot4],set(hotspot[spot4].height,14%); set(hotspot[spot4].width,prop););
		if(hotspot[spot5],set(hotspot[spot5].height,14%); set(hotspot[spot5].width,prop););
		if(hotspot[spot6],set(hotspot[spot6].height,14%); set(hotspot[spot6].width,prop););
		if(hotspot[spot7],set(hotspot[spot7].height,14%); set(hotspot[spot7].width,prop););
		if(hotspot[spot8],set(hotspot[spot8].height,14%); set(hotspot[spot8].width,prop););
		if(hotspot[spot9],set(hotspot[spot9].height,14%); set(hotspot[spot9].width,prop););
		if(hotspot[spot10],set(hotspot[spot10].height,14%); set(hotspot[spot10].width,prop););
		if(hotspot[spot11],set(hotspot[spot11].height,14%); set(hotspot[spot11].width,prop););
		if(hotspot[spot12],set(hotspot[spot12].height,14%); set(hotspot[spot12].width,prop););
		if(hotspot[spot13],set(hotspot[spot13].height,14%); set(hotspot[spot13].width,prop););
		if(hotspot[spot14],set(hotspot[spot14].height,14%); set(hotspot[spot14].width,prop););
		if(hotspot[spot15],set(hotspot[spot15].height,14%); set(hotspot[spot15].width,prop););
		if(hotspot[spot16],set(hotspot[spot16].height,14%); set(hotspot[spot16].width,prop););
	,
		if(hotspot[spot0],set(hotspot[spot0].width,14%); set(hotspot[spot0].height,prop););
		if(hotspot[spot1],set(hotspot[spot1].width,14%); set(hotspot[spot1].height,prop););
		if(hotspot[spot2],set(hotspot[spot2].width,14%); set(hotspot[spot2].height,prop););
		if(hotspot[spot3],set(hotspot[spot3].width,14%); set(hotspot[spot3].height,prop););
