		set(layer[NOVA_Map_Small_Pin_ground].x,get(layer[get(cur_pin)].x));
		set(layer[NOVA_Map_Small_Pin_ground].y,get(layer[get(cur_pin)].y));
		set(layer[NOVA_Map_Small_Pin_3].x,get(layer[get(cur_pin)].x));
		set(layer[NOVA_Map_Small_Pin_3].y,get(layer[get(cur_pin)].y));
		set(layer[NOVA_Map_Small_Pin_4].x,get(layer[get(cur_pin)].x));
		set(layer[NOVA_Map_Small_Pin_4].y,get(layer[get(cur_pin)].y));
		set(layer[NOVA_Map_Small_Pin_roof].x,get(layer[get(cur_pin)].x));
		set(layer[NOVA_Map_Small_Pin_roof].y,get(layer[get(cur_pin)].y));
	</action>
	
