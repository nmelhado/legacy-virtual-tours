hotspot[J Door].fillalpha,0.4); tween(hotspot[J Door].borderalpha,1.0); 		</action>		<action name="Jewelry_Out">			tween(hotspot[J Stand].fillalpha,0.0); tween(hotspot[J Stand].borderalpha,0.0);			tween(hotspot[J Sign].fillalpha,0.0); tween(hotspot[J Sign].borderalpha,0.0); 			tween(hotspot[J Door].fillalpha,0.0); tween(hotspot[J Door].borderalpha,0.0); 		</action>		<hotspot name="J Stand"		style="custom_hotspot"		onover="Jewelry_Over();"		onhover="showtext(' Go To Jewelry Room ')"         	onout="Jewelry_Out();"         	onclick="if(linkedscene, set(enabled,false); skin_hidetoolips(); tween(depth|alpha|oy|rx, 4000|0.0|-50|-60,0.5, default, loadscene(get(linkedscene),null,get(skin_settings.loadscene_flags),get(skin_settings.loadscene_blend));skin_updatescroll();););"		onloaded="if(linkedscene AND skin_settings.tooltips_hotspots, copy(tooltip,scene[get(linkedscene)].title);loadstyle(skin_tooltips););"		linkedscene="scene_Jewelry_Showroom_01"		>		<point ath="-48.922" atv="10.846" />		<point ath="-49.263" atv="10.768" />		<point ath="-49.219" atv="4.438" />		<point ath="-48.315" atv="4.343" />		<point ath="-46.278" atv="4.222" />		<point ath="-43.779" atv="4.160" />		<point ath="-42.201" atv="4.083" />		<point ath="-42.085" atv="10.086" />		<point ath="-45.055" atv="10.460" />		<point ath="-47.344" atv="10.701" />		...	</hotspot>		<hotspot name="J Sign"		style="custom_hotspot"		onover="Jewelry_Over();"		onhover="showtext(' Go To Jewelry Room ')"         	onout="Jewelry_Out();"         	onclick="if(linkedscene, set(enabled,false); skin_hidetoolips(); tween(depth|alpha|oy|rx, 4000|0.0|-50|-60,0.5, default, loadscene(get(linkedscene),null,get(skin_settings.loadscene_flags),get(skin_settings.loadscene_blend));skin_updatescroll();););"		onloaded="if(linkedscene AND skin_settings.tooltips_hotspots, copy(tooltip,scene[get(linkedscene)].title);loadstyle(skin_tooltips););"		linkedscene="scene_Jewelry_Showroom_01"		>		<point ath="-38.33